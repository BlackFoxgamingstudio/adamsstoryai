# Server settings
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Database settings
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/storyboard_db?retryWrites=true&w=majority

# OpenAI API
OPENAI_API_KEY=your_openai_api_key

# Image generation settings
DEFAULT_IMAGE_MODEL=stable_diffusion
STABLE_DIFFUSION_MODEL_PATH=stabilityai/stable-diffusion-XL-base-1.0

# Vector database
VECTOR_DB_TYPE=milvus
VECTOR_DB_URI=your_vector_db_uri
VECTOR_DB_DIM=1024

# Authentication
JWT_SECRET=your_jwt_secret
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis (for Celery)
REDIS_URI=redis://localhost:6379/0 